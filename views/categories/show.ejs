<!doctype html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <div class="page-container">
      <h2 class="page-title"><%= category.name %></h2>
        <div class="quote-wrapper">
        <% quotes.forEach(quote => { %>
          <blockquote><%= quote.author %> says: <%= quote.content %> </blockquote>
          <% }) %>
          </div>
        </div>
    </div>
  </body>
</html>
